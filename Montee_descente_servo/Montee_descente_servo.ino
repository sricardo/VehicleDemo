int SERV = 12 ;int LED = 13 ;bool ok =false ;int duree;void setup() {  pinMode(SERV, OUTPUT);  pinMode(LED, OUTPUT);  Serial.begin(9600);  Serial.println("Initialisation");}void loop() {          Serial.println("Mont√©e");    for (int duree = 950 ; duree <= 2200 ; duree=duree+12) {     digitalWrite(SERV, HIGH);     delayMicroseconds(duree);     digitalWrite(SERV, LOW);     delayMicroseconds(20000-duree);     }          delay(2000);       Serial.println("Descente");    for (int duree = 2200 ; duree >= 950 ; duree=duree-12) {     digitalWrite(SERV, HIGH);     delayMicroseconds(duree);     digitalWrite(SERV, LOW);     delayMicroseconds(20000-duree);     }          delay(2000);   }